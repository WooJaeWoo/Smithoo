<body>
	<aside>
		<div class="profile">
			<div class="picture">
				<img src="/dist/img/smithoo.jpg" alt="">
			</div>
		</div>
		<div class="workshopTitle">
			UI Workshop
		</div>
		<ul class="uiList">
			<li data-ui="ssspin">
				<div class="flipper">
					<div class="front">
						Ssspin
					</div>
					<div class="back">
						Ssspin
					</div>
				</div>
			</li>
			<li data-ui="rrrain">
				<div class="flipper">
					<div class="front">
						Rrrain
					</div>
					<div class="back">
						Rrrain
					</div>
				</div>
			</li>
			<li data-ui="cccard">
				<div class="flipper">
					<div class="front">
						Cccard
					</div>
					<div class="back">
